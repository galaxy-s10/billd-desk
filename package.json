    "billd-scss": "^0.0.8",
    "billd-utils": "^0.0.22",
    "js-cookie": "^3.0.5",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.1",
    "socket.io-client": "^4.8.0",
    "spark-md5": "^3.0.2",
    "vconsole": "^3.15.1",
    "vue": "^3.5.13",
    "vue-i18n": "^9",
    "vue-router": "^4.2.4"
