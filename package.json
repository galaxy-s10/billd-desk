    "prettier": "^3.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.63.6",
    "standard-version": "^9.5.0",
    "trash": "^8.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.25.1",
